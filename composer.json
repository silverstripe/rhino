{
    "name": "silverstripe/installer",
    "type": "silverstripe-recipe",
    "description": "The SilverStripe Framework Installer",
    "require": {
        "php": "^8.3",
        "silverstripe/recipe-cms": "^6",
        "silverstripe/login-forms": "^6",
        "symbiote/silverstripe-queuedjobs": "^6",
        "silverstripe/totp-authenticator": "^6",
        "silverstripe/webauthn-authenticator": "^6",
        "silverstripe/supported-modules": "^2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11"
    },
    "extra": {
        "expose": [
            "themes/rhino/css",
            "themes/rhino/images",
            "themes/rhino/javascript",
            "themes/rhino/tablefilter"
        ],
        "project-files-installed": [
            ".htaccess",
            "app/.htaccess",
            "app/_config/mimevalidator.yml",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "web.config"
        ]
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/src/"
        }
    },
    "config": {
        "process-timeout": 600,
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/recipe-plugin": true,
            "silverstripe/vendor-plugin": true
        },
        "platform": {
            "php": "8.3"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
